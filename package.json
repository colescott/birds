{
  "name": "birds",
  "version": "0.1.0",
  "description": "CardinalBotics Beginning and Intermediate Role Development System.",
  "main": "app.js",
  "readme": "README.md",
  "scripts": {
    "start": "better-npm-run start:prod",
    "start:dev": "better-npm-run start:dev",
    "serve": "better-npm-run serve",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "test": "better-npm-run test",
    "test:api": "better-npm-run test:api",
    "test:api-watch": "better-npm-run test:api --watch",
    "test:fe": "better-npm-run test:fe",
    "test:fe-watch": "better-npm-run test:fe --watch --reporter min",
    "postinstall": "npm run build:prod",
    "lint": "better-npm-run lint",
    "lint:fix": "better-npm-run lint --fix",
    "docs": "better-npm-run docs",
    "cover": "better-npm-run cover",
    "db": "better-npm-run db",
    "analyze": "better-npm-run build:prod --json | webpack-bundle-size-analyzer"
  },
  "precommit": [
    "lint",
    "cover"
  ],
  "betterScripts": {
    "start:prod": {
      "command": "node start.js",
      "env": {
        "NODE_ENV": "PROD"
      }
    },
    "start:dev": {
      "command": "nodemon start.js",
      "env": {
        "NODE_ENV": "DEV",
        "JWT_SECRET": "correcthorsebatterystaple",
        "MONGODB_URI": "mongodb://localhost/birds"
      }
    },
    "serve": {
      "command": "webpack-dev-server --progress --config birds/webpack-dev.config.js --inline --watch",
      "env": {
        "URL_PREFIX": "http://localhost:8000"
      }
    },
    "test": {
      "command": "better-npm-run test:fe && better-npm-run test:api"
    },
    "test:api": {
      "command": "jest birds/test",
      "env": {
        "NODE_ENV": "TEST",
        "JWT_SECRET": "TEST",
        "MONGODB_URI": "mongodb://localhost/test"
      }
    },
    "test:fe": {
      "command": "find ./birds -name '*.test.js' | xargs mocha --opts unit.mocha.opts --timeout 5000",
      "env": {
        "NODE_ENV": "TEST",
        "URL_PREFIX": ""
      }
    },
    "build:dev": {
      "command": "webpack --progress --config birds/webpack-dev.config.js"
    },
    "build:prod": {
      "command": "webpack --progress --config birds/webpack-prod.config.js"
    },
    "lint": {
      "command": "find ./birds -name '*.js' -not -path '*/static/*' -not -path '*/public/*' | xargs eslint"
    },
    "docs": {
      "command": "apidoc -i birds/api -o docs"
    },
    "cover": {
      "command": "istanbul cover _mocha ./birds/**/*.test.js ./birds/test/*.js -- --opts unit.mocha.opts -R spec --timeout 5000",
      "env": {
        "NODE_ENV": "TEST",
        "JWT_SECRET": "meh",
        "MONGODB_URI": "mongodb://localhost/test"
      }
    },
    "db": {
      "command": "mongod --dbpath ./data"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team4159/BIRDS.git"
  },
  "author": "Elias Lawson-Fox <eliaslfox@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Team4159/BIRDS/issues"
  },
  "homepage": "https://github.com/Team4159/BIRDS#readme",
  "dependencies": {
    "apidoc": "0.16.1",
    "aws-sdk": "2.6.14",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-native-modules": "6.9.4",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
